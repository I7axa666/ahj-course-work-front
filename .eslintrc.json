{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": "off",
      "no-alert": "off",
      "import/no-cycle": "off",
      "no-console": "off",
      "func-names": "off",
      "no-continue": "off",
      "max-len": "off",
      "no-return-assign": "off"
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser"

}